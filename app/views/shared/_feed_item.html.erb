<li id="<%=feed_item.id %>">
<span> <%= link_to feed_item.user.username, feed_item.user %></span>
<span> <%= feed_item.status %></span>
<span> <%= time_ago_in_words(feed_item.created_at) %> ago.</span>
<% if feed_item.photo_path.present? %>
		<%= image_tag feed_item.photo_path_url(:thumb).to_s %>
	<% end %>
<% if current_user?(feed_item.user) %>
	<%= link_to "delete", feed_item, method: :delete,
										 data: { confirm: "Are you sure?" },
										 title: feed_item.status %>
<% end %>
</li>