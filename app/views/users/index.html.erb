<%= provide(:title, 'All Users') %>
<h1> All Users </h1>
<ul>
  <%= pluralize(@users.count, "User") %>.
  <hr>

  <% @users.each do |user| %>

      <li>Fullname : <%= user.fullname %></li>
      <li>Username : <%= user.username %></li>
      <li>Email :    <%= user.email %></li>

  <% end %>
  <hr>

</ul>
